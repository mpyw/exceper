{
    "name": "mpyw/exceper",
    "description": "Provides temporary error handler automatically using set_error_handler() and restore_error_handler().",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "set_error_handler",
        "restore_error_handler",
        "error",
        "temporary",
        "ErrorException"
    ],
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Mpyw\\Exceper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\Exceper\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!/src/*"
        ]
    }
}
